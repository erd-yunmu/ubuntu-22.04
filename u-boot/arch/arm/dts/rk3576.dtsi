		mclkout_sai2: mclkout-sai2@26046400 {
			compatible = "rockchip,clk-out";
			reg = <0 0x26046400 0 0x4>;
			clocks = <&cru CLK_SAI2_MCLKOUT>;
			#clock-cells = <0>;
			clock-output-names = "mclk_sai2_to_io";
			rockchip,bit-shift = <2>;
