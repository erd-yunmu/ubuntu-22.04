			reg = <0 0x272008c8 0 0x10>;
			clock-names = "link";
			clocks = <&cru ACLK_VPU_ROOT>;
			#power-domain-cells = <1>;
			#clock-cells = <0>;
		};

		aclk_ebc: aclk_ebc@272008c8 {
			compatible = "rockchip,rk3576-clock-gate-link";
			reg = <0 0x272008c8 0 0x10>;
			clock-names = "link";
