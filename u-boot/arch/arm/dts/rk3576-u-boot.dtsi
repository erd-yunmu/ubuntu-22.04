#include <dt-bindings/gpio/gpio.h>

/ {
	aliases {
		mmc0 = &sdhci;
		mmc1 = &sdmmc;
	};

	chosen {
		stdout-path = &uart0;
		u-boot,spl-boot-order = &sdmmc, &spi_nand, &spi_nor, "same-as-spl";
	};

	secure-otp@2a480000 {
		u-boot,dm-spl;
		compatible = "rockchip,rk3576-secure-otp";
		reg = <0x0 0x2a480000 0x0 0x10000>;
	};
};

&firmware {
	u-boot,dm-pre-reloc;
};

&gpio0 {
	u-boot,dm-spl;
	status = "okay";
};

