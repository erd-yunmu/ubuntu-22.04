				status = "ok";
				logo,uboot = "logo.bmp";
				logo,kernel = "logo_kernel.bmp";
				logo,mode = "center";
				charge_logo,mode = "center";
				connect = <&vp1_out_dp1>;				
			};	

			route_edp0: route-edp0 {
				status = "ok";
