	test-power {
		status = "okay";
	};
};

&pwm0 {
	pinctrl-0 = <&pwm0m0_pins>;
	status = "okay";
};

&pwm1 {
	pinctrl-0 = <&pwm1m0_pins>;
	status = "okay";
};

